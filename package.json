{
  "name": "leiten-zustand",
  "version": "1.4.0",
  "private": false,
  "description": "Zustand state manager controllers",
  "repository": {
    "type": "git",
    "url": "https://github.com:hecmatyar/leiten-zustand.git"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "dist/index.ts",
  "jsnext:main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "check:lint": "eslint src/**/*.{ts,tsx,js}",
    "build:ts": "del-cli dist && tsc --noEmit false",
    "check:ts": "tsc --noEmit",
    "fix:lint": "eslint src/**/*.{ts,tsx,js} --fix",
    "fix:prettier": "prettier --write src/**/*.{json}",
    "sort:package": "npx sort-package-json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "tsc --noEmit"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": []
  },
  "dependencies": {
    "nanoid": "^5.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.31",
    "@types/lodash-es": "^4.17.10",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "clean-publish": "^4.2.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "zustand": "^4.4.4",
    "immer": "^10.0.3",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "react": ">=17",
    "zustand": ">=4",
    "lodash-es": ">=4",
    "nanoid": ">=4",
    "immer": ">=10"
  },
  "engines": {
    "node": ">=17",
    "npm": ">=8"
  },
  "engineStrict": true,
  "publishConfig": {
    "homepage": "https://github.com/Hecmatyar/leiten-zustand"
  },
  "license": "MIT",
  "keywords": [
    "zustand",
    "fetching",
    "state",
    "store",
    "helpers",
    "constrollers",
    "lenses"
  ]
}
